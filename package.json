{
  "name": "@twipped/utils",
  "version": "7.0.0",
  "description": "A collection of commonly used utility functions",
  "type": "module",
  "main": "./index.js",
  "exports": {
    ".": "./index.js",
    "./*": "./src/*.js"
  },
  "scripts": {
    "test": "tap -R base --no-coverage tests",
    "cover": "tap -R terse --coverage tests",
    "lint": "eslint './{src,tests}/**/*.{js,jsx}'",
    "lint-fix": "eslint './{src,tests}/**/*.{js,jsx}' --fix",
    "docs": "jsdoc2md -t README.hbs src/*.js > README.md",
  },
  "engines": {
    "node": ">=14.10.0"
  },
  "author": "Jocelyn Badgley <joc@twipped.com> (http://twipped.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/Twipped/js-utils"
  },
  "devDependencies": {
    "@twipped/eslint-config": "^7.5.2",
    "eslint": "^8.36.0",
    "jsdoc": "^4.0.2",
    "jsdoc-to-markdown": "^8.0.0",
    "pretty-format": "^29.5.0",
    "tap": "^16.3.4",
    "typescript": "^4.9.5"
  },
  "files": [
    "src",
    "index.js",
    "CHANGELOG.md",
    "LICENSE.txt",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
