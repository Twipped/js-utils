{
  "name": "@twipped/utils",
  "version": "0.1.0",
  "description": "A collection of commonly used utility functions",
  "main": "dist/utils.cjs.js",
  "module": "dist/utils.esm.js",
  "scripts": {
    "test": "tap -R base --no-coverage tests",
    "cover": "tap -R terse --coverage tests",
    "exe": "babel-node --presets @babel/env",
    "lint": "eslint './{src,tests}/**/*.{js,jsx}' --fix",
    "build": "rollup -c",
    "clean": "rm -rf ./dist ./.nyc_output ./util.js",
    "start": "npm run lint && npm run clean && npm run build"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "author": "Jocelyn Badgley <joc@twipped.com> (http://twipped.com)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@babel/node": "~7.8.7",
    "@babel/preset-env": "~7.9.5",
    "@rollup/plugin-node-resolve": "~7.1.3",
    "babel-eslint": "~10.1.0",
    "babel-node": "0.0.1-security",
    "date-fns": "~2.13.0",
    "eslint": "~6.8.0",
    "eslint-config-twipped": "~3.4.0",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-promise": "~4.2.1",
    "marky": "~1.2.1",
    "rollup": "~2.7.2",
    "rollup-plugin-babel": "~4.4.0",
    "rollup-plugin-banner": "~0.2.1",
    "rollup-plugin-terser": "~5.3.0",
    "tap": "~14.10.5"
  },
  "files": [
    "dist",
    "src",
    "CHANGELOG.md",
    "LICENSE.txt",
    "README.md"
  ],
  "dependencies": {}
}
